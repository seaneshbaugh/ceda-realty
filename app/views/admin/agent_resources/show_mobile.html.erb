<h2>Name:</h2>
<div><%= @agent_resource.name %></div>
<h2>Category:</h2>
<div><%= @agent_resource.category %></div>
<h2>Description:</h2>
<div><%= @agent_resource.description %></div>
<h2>Original Filename:</h2>
<div><%= @agent_resource.original_filename %></div>
<h2>Resource Filename:</h2>
<div><%= @agent_resource.resource_filename %></div>
<h2>Extension:</h2>
<div><%= @agent_resource.extension %></div>
<h2>SHA2 Checksum:</h2>
<div><%= @agent_resource.sha2_checksum %></div>
<h2>External URL:</h2>
<div><%= @agent_resource.extenal_url %></div>
<h2>Created at: <%= @agent_resource.created_at.strftime("%Y-%m-%d %H:%M:%S") %></h2>
<h2>Last Updated at: <%= @agent_resource.updated_at.strftime("%Y-%m-%d %H:%M:%S") %></h2>
<% if !@agent_resource.resource_filename.blank? %>
	<h2><%= link_to("Download", get_file_admin_agent_resource_path(@agent_resource)) %></h2>
<% end %>
<% if !@agent_resource.external_url.blank? %>
	<h2><%= link_to("Visit Link", @agent_resource.external_url) %></h2>
<% end %>
<% if @user == @person or @user.privilege_level > @person.privilege_level %>
<div>
	<a onclick="return loadpage('<%= edit_admin_agent_resource_path(@agent_resource) %>');">Edit Agent Resource</a>
	<%= link_to("Delete User", admin_agent_resource_path(@agent_resource), :method => :delete, :confirm => "Are you sure you want to delete this agent resource? This action cannot be undone!") %>
</div>

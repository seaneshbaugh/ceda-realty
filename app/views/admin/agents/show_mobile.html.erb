<h2>ID:</h2>
<div><%= @agent.id %></div>
<h2>User ID:</h2>
<div><%= @agent.person_id %></div>
<h2>Picture ID:</h2>
<div><%= @agent.picture_id %></div>
<h2>Title:</h2>
<div><%= @agent.title %></div>
<h2>Display Email Address:</h2>
<div><%= @agent.display_email_address %></div>
<h2>Display Phone Number:</h2>
<div><%= @agent.display_phone_number %></div>
<h2>Website URL:</h2>
<div><%= @agent.website_url %></div>
<h2>Facebook URL:</h2>
<div><%= @agent.facebook_url %></div>
<h2>Twitter URL:</h2>
<div><%= @agent.twitter_url %></div>
<h2>Linked-In URL:</h2>
<div><%= @agent.linked_in_url %></div>
<h2>Active Rain URL:</h2>
<div><%= @agent.active_rain_url %></div>
<h2>Youtube URL:</h2>
<div><%= @agent.youtube_url %></div>
<h2>Bio Body:</h2>
<div><%= h(@agent.bio_body) %></div>
<h2>Bio Style:</h2>
<div><%= @agent.bio_style %></div>
<h2>MLS Search Name 1:</h2>
<div><%= @agent.mls_name1 %></div>
<h2>MLS Search Name 2:</h2>
<div><%= @agent.mls_name2 %></div>
<h2>Created at: <%= @agent.created_at.strftime("%Y-%m-%d %H:%M:%S") %></h2>
<h2>Last Updated at: <%= @agent.updated_at.strftime("%Y-%m-%d %H:%M:%S") %></h2>
<div>
	<a onclick="return loadpage('<%= edit_agent_page_path(@agent) %>');">Edit User</a>
	<%= link_to("Delete User", admin_agent_path(@agent), :method => :delete, :confirm => "Are you sure you want to delete this agent? This action cannot be undone!") %>
</div>
